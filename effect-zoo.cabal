  ghc-options: -O2 -flate-specialise

  default-extensions:
    BangPatterns DeriveGeneric GADTs GeneralizedNewtypeDeriving KindSignatures
    LambdaCase PackageImports ScopedTypeVariables TemplateHaskell TypeOperators
